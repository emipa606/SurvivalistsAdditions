<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ProcessorFramework.ProcessDef>
    <defName>SRV_Charcoal_Process</defName>
    <thingDef>SRV_Charcoal</thingDef>
    <ingredientFilter>
      <thingDefs>
        <li>WoodLog</li>
      </thingDefs>
    </ingredientFilter>
    <usesTemperature>false</usesTemperature>
    <processDays>4</processDays>
  </ProcessorFramework.ProcessDef>
  <ProcessorFramework.ProcessDef>
    <defName>SRV_SmokedCheese_Process</defName>
    <thingDef>SRV_SmokedCheese</thingDef>
    <ingredientFilter>
      <thingDefs>
        <li>SRV_Cheese</li>
      </thingDefs>
    </ingredientFilter>
    <processDays>2</processDays>
    <temperatureSafe>-1.0~35.0</temperatureSafe>
  </ProcessorFramework.ProcessDef>
  <ProcessorFramework.ProcessDef>
    <defName>SRV_SmokedMeat_Process</defName>
    <thingDef>SRV_SmokedMeat</thingDef>
    <ingredientFilter>
      <categories>
        <li>MeatRaw</li>
      </categories>
      <disallowedThingDefs>
        <li>SRV_SmokedMeat</li>
      </disallowedThingDefs>
    </ingredientFilter>
    <processDays>2</processDays>
    <temperatureSafe>-1.0~35.0</temperatureSafe>
  </ProcessorFramework.ProcessDef>
  <ProcessorFramework.ProcessDef>
    <defName>SRV_Cheese_Process</defName>
    <thingDef>SRV_Cheese</thingDef>
    <ingredientFilter>
      <thingDefs>
        <li>SRV_CurdledMilk</li>
      </thingDefs>
    </ingredientFilter>
    <usesTemperature>true</usesTemperature>
    <temperatureSafe>-1.0~32.0</temperatureSafe>
    <temperatureIdeal>7.0~32.0</temperatureIdeal>
    <ruinedPerDegreePerHour>2.5</ruinedPerDegreePerHour>
    <speedBelowSafe>0.1</speedBelowSafe>
    <speedAboveSafe>1.0</speedAboveSafe>
    <processDays>6</processDays>
    <efficiency>1.0</efficiency>
    <sunFactor>1.0~1.0</sunFactor>
    <rainFactor>1.0~1.0</rainFactor>
    <snowFactor>1.0~1.0</snowFactor>
    <windFactor>1.0~1.0</windFactor>
    <usesQuality>false</usesQuality>
    <qualityDays>(1, 2, 3, 4, 5, 6, 7)</qualityDays>
  </ProcessorFramework.ProcessDef>
  <ProcessorFramework.ProcessDef>
    <defName>SRV_Vinegar_Process</defName>
    <thingDef>SRV_Vinegar</thingDef>
    <ingredientFilter>
      <thingDefs>
        <li>SRV_VinegarJuice</li>
      </thingDefs>
    </ingredientFilter>
    <usesTemperature>true</usesTemperature>
    <temperatureSafe>-1.0~32.0</temperatureSafe>
    <temperatureIdeal>7.0~32.0</temperatureIdeal>
    <ruinedPerDegreePerHour>2.5</ruinedPerDegreePerHour>
    <speedBelowSafe>0.1</speedBelowSafe>
    <speedAboveSafe>1.0</speedAboveSafe>
    <processDays>6</processDays>
    <efficiency>1.0</efficiency>
    <sunFactor>1.0~1.0</sunFactor>
    <rainFactor>1.0~1.0</rainFactor>
    <snowFactor>1.0~1.0</snowFactor>
    <windFactor>1.0~1.0</windFactor>
    <usesQuality>false</usesQuality>
    <qualityDays>(1, 2, 3, 4, 5, 6, 7)</qualityDays>
  </ProcessorFramework.ProcessDef>
</Defs>
